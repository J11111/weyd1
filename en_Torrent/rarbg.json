{
    "name": "rarbg",
    "language": ["hi", "en"],
    "domains": ["torrentapi.org", "rarbgproxied.org","rarbgget.org"],
    "base_url": "https://torrentapi.org",
    "token": {
        "query": "/pubapi_v2.php?get_token=get_token&app_id=wako",
        "token_validity_time_ms": 840000,
        "token_format": {
        "token": "token"
        }
    },
    "search_url_format_episode": {
      "string_format": "/pubapi_v2.php?mode=search&search_string=%s+s%02de%02d&category=tv&format=json_extended&app_id=wako",
      "replacement": [
        "title_lower",
        "season_int",
        "episode_int"
      ]
    },
    "search_url_format_movie": {
      "string_format": "/pubapi_v2.php?mode=search&search_string=%s+%04d&category=movies&format=json_extended&app_id=wako",
      "replacement": [
        "title_lower",
        "year_int"
      ]
    },
    "search_url_format_season_pack": {
      "string_format": "/pubapi_v2.php?mode=search&search_string=%s&category=tv&format=json_extended&app_id=wako",
      "replacement": [
        "title_lower"
      ]
    },
    "api":{
        "path_to_array": "torrent_results",
        "path_title": "title",
        "path_seeders": "seeders",
        "path_size": "size",
        "path_magnet": "download"
    },
    "is_torrent": true,
    "is_direct": false
  }
